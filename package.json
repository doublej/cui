{
  "name": "cui-agentsdk-svelte",
  "version": "0.1.0",
  "type": "module",
  "description": "Svelte-based Agent UI Platform using Claude Agent SDK",
  "keywords": [
    "claude",
    "claude-agent-sdk",
    "svelte",
    "sveltekit",
    "ai",
    "agent"
  ],
  "author": "Wenbo Pan",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/doublej/cui.git"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "NODE_ENV=test vitest run",
    "test:debug": "NODE_ENV=test LOG_LEVEL=debug vitest run",
    "unit-tests": "NODE_ENV=test vitest run tests/unit",
    "integration-tests": "NODE_ENV=test vitest run tests/integration",
    "test:watch": "NODE_ENV=test vitest",
    "test:coverage": "NODE_ENV=test vitest run --coverage",
    "lint": "eslint .",
    "typecheck": "svelte-kit sync && tsc --noEmit"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.76",
    "@anthropic-ai/sdk": "^0.54.0",
    "@google/genai": "^1.11.0",
    "@melt-ui/svelte": "^0.86.0",
    "better-sqlite3": "^12.2.0",
    "clsx": "^2.1.1",
    "diff": "^8.0.2",
    "lucide-svelte": "^0.469.0",
    "pino": "^8.17.1",
    "prismjs": "^1.30.0",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/kit": "^2.21.1",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.11",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.19.1",
    "@types/prismjs": "^1.26.5",
    "@types/uuid": "^9.0.7",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.32.0",
    "eslint-plugin-svelte": "^3.9.0",
    "svelte": "^5.33.0",
    "svelte-check": "^4.2.1",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.3.3",
    "vite": "^7.0.6",
    "vitest": "^3.2.4"
  },
  "optionalDependencies": {
    "@tailwindcss/oxide-linux-x64-gnu": "^4.1.11",
    "lightningcss-linux-x64-gnu": "^1.30.1"
  }
}
